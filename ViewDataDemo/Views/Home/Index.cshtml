
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<h2>
    @ViewData["Message"]
</h2>
<h2>
    @ViewData["CurrentTime"]
</h2>

<ul>
    @{
        foreach (string i in (string[])ViewData["FruitsArray"])
        {
            <li>
                @i
            </li>
        }
    }

</ul>
<ul>
    @{
        foreach (string i in (List<string>)ViewData["SportsList"])
        {
            <li>
                @i
            </li>
        }
    }

</ul>
@{ 
    var a = (ViewDataDemo.Models.Employee)ViewData["Employee"];
}

<p>@a.EmpID</p>
<p>@a.EmpName</p>
<p>@a.EmpDesignation</p>